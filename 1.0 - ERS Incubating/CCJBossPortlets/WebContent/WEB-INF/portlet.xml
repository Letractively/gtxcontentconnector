<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
             version="2.0">
   <portlet>
       <portlet-name>CCJBossContentPortlet</portlet-name>
           <portlet-class>com.gentics.cr.portlet.CCJBossContentPortlet</portlet-class>
           <init-param>
			   <name>startpage</name>
			   <value>10007.938</value>
			</init-param>
			<init-param>
			   <name>template</name>
			   <value>/WEB-INF/template/content/content.jsp</value>
			</init-param>
           <supports>
           <mime-type>text/html</mime-type>
           <portlet-mode>view</portlet-mode>
           <portlet-mode>edit</portlet-mode>
           <portlet-mode>help</portlet-mode>
           </supports>
           <portlet-info>
               <title>Content Portlet</title>               
           </portlet-info>
           <supported-processing-event>
           	<name>GENTICS_CONTENT</name>
           </supported-processing-event>
       </portlet>
   <portlet>
       <portlet-name>CCJBossNavigationPortlet</portlet-name>
           <portlet-class>com.gentics.cr.portlet.CCJBossNavigationPortlet</portlet-class>
           <init-param>
			   <name>startfolder</name>
			   <value>10002.126</value>
			</init-param>
			<init-param>
			   <name>template</name>
			   <value>/WEB-INF/template/tree.vm</value>
			</init-param>
			<supports>
           <mime-type>text/html</mime-type>
           <portlet-mode>view</portlet-mode>
           <portlet-mode>edit</portlet-mode>
           <portlet-mode>help</portlet-mode>
           </supports>
           <portlet-info>
               <title>Navigation Portlet</title>               
           </portlet-info>
           <supported-publishing-event>
           		<name>GENTICS_CONTENT</name>
           </supported-publishing-event>
       </portlet>
       <event-definition>
		<name>GENTICS_CONTENT</name>
	</event-definition>
</portlet-app>
